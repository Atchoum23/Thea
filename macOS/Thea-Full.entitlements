<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!--
    FULL ENTITLEMENTS FILE FOR DIRECT DISTRIBUTION (Developer ID)

    Use this file AFTER you receive Apple approval for:
    - Network Extension (content filter, VPN)
    - Endpoint Security
    - System Extension

    To request these capabilities:
    1. Go to https://developer.apple.com/contact/request/network-extension
    2. Fill out the form explaining your use case
    3. Wait for approval (typically 1-2 weeks)
    4. Enable capabilities in App ID at developer.apple.com/account
    5. Regenerate provisioning profiles
    6. Replace Thea.entitlements with this file
-->
<plist version="1.0">
<dict>
	<!-- ================================ -->
	<!-- Push Notifications               -->
	<!-- ================================ -->
	<key>com.apple.developer.aps-environment</key>
	<string>development</string>

	<!-- ================================ -->
	<!-- iCloud Services                  -->
	<!-- ================================ -->
	<key>com.apple.developer.icloud-container-identifiers</key>
	<array>
		<string>iCloud.app.theathe</string>
	</array>
	<key>com.apple.developer.icloud-services</key>
	<array>
		<string>CloudDocuments</string>
		<string>CloudKit</string>
	</array>
	<key>com.apple.developer.ubiquity-kvstore-identifier</key>
	<string>$(TeamIdentifierPrefix)app.theathe</string>

	<!-- ================================ -->
	<!-- App Groups                       -->
	<!-- ================================ -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.app.theathe</string>
	</array>

	<!-- ================================ -->
	<!-- File Access                      -->
	<!-- ================================ -->
	<key>com.apple.security.files.bookmarks.app-scope</key>
	<true/>
	<key>com.apple.security.files.bookmarks.document-scope</key>
	<true/>
	<key>com.apple.security.temporary-exception.files.home-relative-path.read-only</key>
	<array>
		<string>/Library/</string>
	</array>

	<!-- ================================ -->
	<!-- Speech Recognition               -->
	<!-- ================================ -->
	<key>com.apple.security.personal-information.speech-recognition</key>
	<true/>

	<!-- ================================ -->
	<!-- Scripting Targets                -->
	<!-- ================================ -->
	<key>com.apple.security.scripting-targets</key>
	<dict>
		<key>com.apple.Notes</key>
		<array>
			<string>com.apple.notes.text</string>
		</array>
		<key>com.apple.Safari</key>
		<array>
			<string>com.apple.safari.browsing</string>
		</array>
		<key>com.apple.finder</key>
		<array>
			<string>com.apple.finder.file-operations</string>
		</array>
		<key>com.apple.iCal</key>
		<array>
			<string>com.apple.ical.access</string>
		</array>
		<key>com.apple.mail</key>
		<array>
			<string>com.apple.mail.compose</string>
		</array>
		<key>com.apple.reminders</key>
		<array>
			<string>com.apple.reminders.access</string>
		</array>
		<key>com.apple.systemevents</key>
		<array>
			<string>com.apple.systemevents.processes</string>
		</array>
	</dict>

	<!-- ================================ -->
	<!-- Accessibility (Automation)       -->
	<!-- ================================ -->
	<key>com.apple.security.automation.apple-events</key>
	<true/>

	<!-- ================================ -->
	<!-- Hardened Runtime Exceptions      -->
	<!-- ================================ -->
	<key>com.apple.security.cs.allow-unsigned-executable-memory</key>
	<true/>
	<key>com.apple.security.cs.disable-library-validation</key>
	<true/>

	<!-- ================================ -->
	<!-- Network Extension                -->
	<!-- ================================ -->
	<key>com.apple.developer.networking.networkextension</key>
	<array>
		<string>content-filter-provider</string>
		<string>content-filter-provider-system</string>
		<string>packet-tunnel-provider</string>
		<string>packet-tunnel-provider-system</string>
		<string>dns-proxy</string>
		<string>dns-settings</string>
	</array>

	<!-- ================================ -->
	<!-- Endpoint Security                -->
	<!-- ================================ -->
	<key>com.apple.developer.endpoint-security.client</key>
	<true/>

	<!-- ================================ -->
	<!-- System Extension                 -->
	<!-- ================================ -->
	<key>com.apple.developer.system-extension.install</key>
	<true/>
</dict>
</plist>
