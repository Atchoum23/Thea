<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- App Groups (for sharing data with widgets and extensions) -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.app.theathe</string>
	</array>

	<!-- HealthKit -->
	<key>com.apple.developer.healthkit</key>
	<true/>
	<key>com.apple.developer.healthkit.access</key>
	<array>
		<string>health-records</string>
	</array>
	<key>com.apple.developer.healthkit.background-delivery</key>
	<true/>

	<!-- Siri -->
	<key>com.apple.developer.siri</key>
	<true/>

	<!-- Push Notifications -->
	<key>aps-environment</key>
	<string>development</string>

	<!-- iCloud -->
	<key>com.apple.developer.icloud-container-identifiers</key>
	<array>
		<string>iCloud.app.theathe</string>
	</array>
	<key>com.apple.developer.icloud-services</key>
	<array>
		<string>CloudDocuments</string>
		<string>CloudKit</string>
	</array>
	<key>com.apple.developer.ubiquity-kvstore-identifier</key>
	<string>$(TeamIdentifierPrefix)app.theathe</string>

	<!-- Associated Domains (for universal links) -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:theathe.app</string>
	</array>

	<!-- Network Capabilities for Remote Server/Client -->
	<key>com.apple.developer.networking.multicast</key>
	<true/>

	<!-- Background Modes are configured in Info.plist, not entitlements -->
</dict>
</plist>
