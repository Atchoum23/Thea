<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!--
    FULL ENTITLEMENTS FILE FOR DIRECT DISTRIBUTION

    Use this file AFTER you receive Apple approval for:
    - Network Extension (content filter, VPN)
    - Credentials Provider (Password AutoFill)

    To request these capabilities:
    1. Go to https://developer.apple.com/contact/request/network-extension
    2. Fill out the form explaining your use case
    3. Wait for approval (typically 1-2 weeks)
    4. Enable capabilities in App ID at developer.apple.com/account
    5. Regenerate provisioning profiles
    6. Replace Thea.entitlements with this file
-->
<plist version="1.0">
<dict>
	<!-- ================================ -->
	<!-- App Groups                       -->
	<!-- ================================ -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.app.theathe</string>
	</array>

	<!-- ================================ -->
	<!-- HealthKit                        -->
	<!-- ================================ -->
	<key>com.apple.developer.healthkit</key>
	<true/>
	<key>com.apple.developer.healthkit.access</key>
	<array>
		<string>health-records</string>
	</array>
	<key>com.apple.developer.healthkit.background-delivery</key>
	<true/>
	<key>com.apple.developer.healthkit.recalibrate-estimates</key>
	<true/>

	<!-- ================================ -->
	<!-- Siri & Shortcuts                 -->
	<!-- ================================ -->
	<key>com.apple.developer.siri</key>
	<true/>

	<!-- ================================ -->
	<!-- Push Notifications               -->
	<!-- ================================ -->
	<key>aps-environment</key>
	<string>development</string>

	<!-- ================================ -->
	<!-- iCloud Services                  -->
	<!-- ================================ -->
	<key>com.apple.developer.icloud-container-identifiers</key>
	<array>
		<string>iCloud.app.theathe</string>
	</array>
	<key>com.apple.developer.icloud-services</key>
	<array>
		<string>CloudDocuments</string>
		<string>CloudKit</string>
	</array>
	<key>com.apple.developer.ubiquity-kvstore-identifier</key>
	<string>$(TeamIdentifierPrefix)app.theathe</string>

	<!-- ================================ -->
	<!-- Associated Domains               -->
	<!-- ================================ -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:theathe.app</string>
		<string>webcredentials:theathe.app</string>
		<string>activitycontinuation:theathe.app</string>
	</array>

	<!-- ================================ -->
	<!-- HomeKit                          -->
	<!-- ================================ -->
	<key>com.apple.developer.homekit</key>
	<true/>

	<!-- ================================ -->
	<!-- Default Data Protection          -->
	<!-- ================================ -->
	<key>com.apple.developer.default-data-protection</key>
	<string>NSFileProtectionComplete</string>

	<!-- ================================ -->
	<!-- Network - WiFi Info              -->
	<!-- ================================ -->
	<key>com.apple.developer.networking.wifi-info</key>
	<true/>

	<!-- ================================ -->
	<!-- Network Extensions               -->
	<!-- ================================ -->
	<key>com.apple.developer.networking.networkextension</key>
	<array>
		<string>content-filter-provider</string>
		<string>packet-tunnel-provider</string>
		<string>dns-proxy</string>
		<string>dns-settings</string>
	</array>

	<!-- ================================ -->
	<!-- Inter-App Audio                  -->
	<!-- ================================ -->
	<key>inter-app-audio</key>
	<true/>

	<!-- ================================ -->
	<!-- App Attest (Security)            -->
	<!-- ================================ -->
	<key>com.apple.developer.devicecheck.appattest-environment</key>
	<string>development</string>

	<!-- ================================ -->
	<!-- Communication Notifications      -->
	<!-- ================================ -->
	<key>com.apple.developer.usernotifications.communication</key>
	<true/>

	<!-- ================================ -->
	<!-- Time Sensitive Notifications     -->
	<!-- ================================ -->
	<key>com.apple.developer.usernotifications.time-sensitive</key>
	<true/>

	<!-- ================================ -->
	<!-- Group Activities (SharePlay)     -->
	<!-- ================================ -->
	<key>com.apple.developer.group-session</key>
	<true/>

	<!-- ================================ -->
	<!-- Keychain Sharing                 -->
	<!-- ================================ -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)app.thea.ios</string>
		<string>$(AppIdentifierPrefix)group.app.theathe</string>
	</array>

	<!-- ================================ -->
	<!-- Credentials Provider             -->
	<!-- ================================ -->
	<key>com.apple.developer.authentication-services.autofill-credential-provider</key>
	<true/>

	<!-- ================================ -->
	<!-- Family Controls / Screen Time    -->
	<!-- ================================ -->
	<key>com.apple.developer.family-controls</key>
	<true/>

	<!-- ================================ -->
	<!-- Extended Virtual Addressing      -->
	<!-- ================================ -->
	<key>com.apple.developer.kernel.extended-virtual-addressing</key>
	<true/>
</dict>
</plist>
