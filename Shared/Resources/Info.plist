<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- ================================ -->
	<!-- Bundle Information               -->
	<!-- ================================ -->
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Thea</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Thea</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.5.0</string>
	<key>CFBundleVersion</key>
	<string>15</string>

	<!-- ================================ -->
	<!-- macOS Specific                   -->
	<!-- ================================ -->
	<key>LSMinimumSystemVersion</key>
	<string>$(MACOSX_DEPLOYMENT_TARGET)</string>
	<key>NSHumanReadableCopyright</key>
	<string>Copyright Â© 2026 Thea. All rights reserved.</string>
	<key>NSMainStoryboardFile</key>
	<string></string>
	<key>NSPrincipalClass</key>
	<string>NSApplication</string>
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.developer-tools</string>
	<key>NSSupportsAutomaticTermination</key>
	<false/>
	<key>NSSupportsSuddenTermination</key>
	<false/>

	<!-- ================================ -->
	<!-- Privacy Usage Descriptions       -->
	<!-- ================================ -->
	<!-- Microphone -->
	<key>NSMicrophoneUsageDescription</key>
	<string>Thea uses the microphone for voice conversations and commands.</string>

	<!-- Camera -->
	<key>NSCameraUsageDescription</key>
	<string>Thea uses the camera for visual analysis and image capture.</string>

	<!-- Speech Recognition -->
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>Thea uses speech recognition to understand your voice commands.</string>

	<!-- Location -->
	<key>NSLocationUsageDescription</key>
	<string>Thea uses your location for contextual assistance and location-based reminders.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Thea uses your location for contextual assistance.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Thea uses your location to provide proactive assistance and location-based reminders.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Thea uses background location for location-based reminders and context.</string>

	<!-- Contacts -->
	<key>NSContactsUsageDescription</key>
	<string>Thea accesses your contacts to help you communicate and manage relationships.</string>

	<!-- Calendars -->
	<key>NSCalendarsUsageDescription</key>
	<string>Thea reads and manages your calendar events for scheduling assistance.</string>
	<key>NSCalendarsWriteOnlyAccessUsageDescription</key>
	<string>Thea creates calendar events on your behalf.</string>
	<key>NSCalendarsFullAccessUsageDescription</key>
	<string>Thea manages your calendar for comprehensive scheduling assistance.</string>

	<!-- Reminders -->
	<key>NSRemindersUsageDescription</key>
	<string>Thea creates and manages reminders to help you stay organized.</string>
	<key>NSRemindersFullAccessUsageDescription</key>
	<string>Thea manages your reminders for comprehensive task assistance.</string>

	<!-- Photos -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Thea accesses your photos for organization, search, and visual analysis.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Thea saves images and screenshots to your photo library.</string>

	<!-- Health -->
	<key>NSHealthShareUsageDescription</key>
	<string>Thea reads your health data to provide wellness insights and tracking.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Thea writes health data you authorize to support your wellness goals.</string>
	<key>NSHealthClinicalHealthRecordsShareUsageDescription</key>
	<string>Thea accesses clinical records to provide comprehensive health insights.</string>

	<!-- HomeKit -->
	<key>NSHomeKitUsageDescription</key>
	<string>Thea controls your smart home devices and scenes.</string>

	<!-- Apple Music -->
	<key>NSAppleMusicUsageDescription</key>
	<string>Thea accesses your music library for playback control.</string>

	<!-- Motion & Fitness -->
	<key>NSMotionUsageDescription</key>
	<string>Thea tracks your activity to provide fitness insights.</string>

	<!-- Face ID -->
	<key>NSFaceIDUsageDescription</key>
	<string>Authenticate securely with Face ID.</string>

	<!-- Bluetooth -->
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Thea uses Bluetooth for device connectivity and continuity features.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Thea connects to Bluetooth devices for enhanced functionality.</string>

	<!-- Local Network -->
	<key>NSLocalNetworkUsageDescription</key>
	<string>Thea uses the local network for device discovery and remote control features.</string>
	<key>NSBonjourServices</key>
	<array>
		<string>_thea._tcp</string>
	</array>

	<!-- Screen Capture (macOS) -->
	<key>NSScreenCaptureUsageDescription</key>
	<string>Thea captures screen content to assist with visual tasks and screen sharing.</string>

	<!-- System Events / Automation (macOS) -->
	<key>NSAppleEventsUsageDescription</key>
	<string>Thea automates applications to complete tasks on your behalf.</string>

	<!-- Accessibility (macOS) -->
	<key>NSAccessibilityUsageDescription</key>
	<string>Thea reads screen content and controls UI elements to provide intelligent assistance.</string>

	<!-- System Administration (macOS) -->
	<key>NSSystemAdministrationUsageDescription</key>
	<string>Thea requires admin access for system monitoring and automation.</string>

	<!-- Desktop Folder (macOS) -->
	<key>NSDesktopFolderUsageDescription</key>
	<string>Thea accesses your Desktop folder for file management.</string>

	<!-- Documents Folder (macOS) -->
	<key>NSDocumentsFolderUsageDescription</key>
	<string>Thea accesses your Documents folder for file management.</string>

	<!-- Downloads Folder (macOS) -->
	<key>NSDownloadsFolderUsageDescription</key>
	<string>Thea accesses your Downloads folder for file management.</string>

	<!-- Network Volumes (macOS) -->
	<key>NSNetworkVolumesUsageDescription</key>
	<string>Thea accesses network volumes for file management.</string>

	<!-- Removable Volumes (macOS) -->
	<key>NSRemovableVolumesUsageDescription</key>
	<string>Thea accesses removable drives for file management.</string>

	<!-- ================================ -->
	<!-- App Transport Security           -->
	<!-- ================================ -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>

	<!-- ================================ -->
	<!-- Export Compliance                -->
	<!-- ================================ -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>

	<!-- ================================ -->
	<!-- URL Schemes                      -->
	<!-- ================================ -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>app.thea</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>thea</string>
			</array>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
		</dict>
	</array>

	<!-- ================================ -->
	<!-- Document Types                   -->
	<!-- ================================ -->
	<key>CFBundleDocumentTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeName</key>
			<string>Thea Conversation</string>
			<key>CFBundleTypeExtensions</key>
			<array>
				<string>theaconv</string>
			</array>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>LSHandlerRank</key>
			<string>Owner</string>
		</dict>
	</array>

	<!-- ================================ -->
	<!-- macOS Services Menu              -->
	<!-- ================================ -->
	<key>NSServices</key>
	<array>
		<!-- Ask Thea about selection -->
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Ask Thea about Selection</string>
			</dict>
			<key>NSMessage</key>
			<string>askTheaAboutSelection</string>
			<key>NSPortName</key>
			<string>Thea</string>
			<key>NSSendTypes</key>
			<array>
				<string>public.plain-text</string>
				<string>public.rtf</string>
				<string>public.html</string>
			</array>
			<key>NSKeyEquivalent</key>
			<dict>
				<key>default</key>
				<string>t</string>
			</dict>
		</dict>
		<!-- Summarize with Thea -->
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Summarize with Thea</string>
			</dict>
			<key>NSMessage</key>
			<string>summarizeWithThea</string>
			<key>NSPortName</key>
			<string>Thea</string>
			<key>NSSendTypes</key>
			<array>
				<string>public.plain-text</string>
				<string>public.rtf</string>
				<string>public.html</string>
			</array>
			<key>NSKeyEquivalent</key>
			<dict>
				<key>default</key>
				<string>s</string>
			</dict>
		</dict>
		<!-- Translate with Thea -->
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Translate with Thea</string>
			</dict>
			<key>NSMessage</key>
			<string>translateWithThea</string>
			<key>NSPortName</key>
			<string>Thea</string>
			<key>NSSendTypes</key>
			<array>
				<string>public.plain-text</string>
				<string>public.rtf</string>
			</array>
		</dict>
		<!-- Add to Thea Memory -->
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Add to Thea Memory</string>
			</dict>
			<key>NSMessage</key>
			<string>addToTheaMemory</string>
			<key>NSPortName</key>
			<string>Thea</string>
			<key>NSSendTypes</key>
			<array>
				<string>public.plain-text</string>
				<string>public.rtf</string>
				<string>public.html</string>
				<string>public.url</string>
			</array>
		</dict>
		<!-- Explain Code with Thea -->
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Explain Code with Thea</string>
			</dict>
			<key>NSMessage</key>
			<string>explainCodeWithThea</string>
			<key>NSPortName</key>
			<string>Thea</string>
			<key>NSSendTypes</key>
			<array>
				<string>public.plain-text</string>
				<string>public.source-code</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
